{
	"name": "df_limpiar_llamadas",
	"properties": {
		"folder": {
			"name": "SILVER"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_llamadas_entel",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "cast1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast2"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "derivedColumn4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SERVICIO as string,",
				"          REGISTRO as string,",
				"          NUMERO_A as string,",
				"          IMEI_A as string,",
				"          RBS_UTILIZADA_A as string,",
				"          CELDA_A as string,",
				"          LATITUD_LONGITUD_A as string,",
				"          NUMERO_B as string,",
				"          IMEI_B as string,",
				"          RBS_UTILIZADA_B as string,",
				"          CELDA_B as string,",
				"          LATITUD_LONGITUD_B as string,",
				"          FECHA as string,",
				"          DURACION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 cast(output(",
				"          FECHA as timestamp 'dd-MM-yyyy HH:mm:ss'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 derive(DURACION = replace(DURACION,'-', '00:00:00')) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          DURACION as timestamp 'HH:mm:ss'",
				"     ),",
				"     errors: true) ~> cast2",
				"cast2 derive(Latitud_a = substring(LATITUD_LONGITUD_A, 0, instr(LATITUD_LONGITUD_A, \",-\")-1),",
				"          Longitud_a = substring(LATITUD_LONGITUD_A,instr(LATITUD_LONGITUD_A, \",-\")+1,length(LATITUD_LONGITUD_A))\r",
				"\r",
				") ~> derivedColumn2",
				"derivedColumn2 derive(Latitud_b = substring(LATITUD_LONGITUD_B, 0, instr(LATITUD_LONGITUD_B, \",-\")-1),",
				"          Longitud_b = substring(LATITUD_LONGITUD_B,instr(LATITUD_LONGITUD_B, \",-\")+1,length(LATITUD_LONGITUD_B))) ~> derivedColumn3",
				"derivedColumn3 derive(Latitud_a = replace(replace(Latitud_a, '', '0.0'),'-','0.0'),",
				"          Longitud_a = replace(replace(Longitud_a, '', '0.0'),'-','0.0'),",
				"          Latitud_b = replace(replace(Latitud_b, '', '0.0'),'-','0.0'),",
				"          Longitud_b = replace(replace(Longitud_b, '', '0.0'),'-','0.0')) ~> derivedColumn4"
			]
		}
	}
}