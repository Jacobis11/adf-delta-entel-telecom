{
	"name": "df_hechos_datos",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				},
				{
					"name": "Dimorigen"
				},
				{
					"name": "Dimfecha"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_datos",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Dimorigen",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Dimfecha",
				"source1, Dimorigen lookup(source1@NUMERO == Dimorigen@numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, Dimfecha lookup(FECHA_KEY == fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2 select(mapColumn(",
				"          id_origen_cinco,",
				"          id_fecha_cinco",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_sesion_cinco as integer,",
				"          fecha_inicio as timestamp,",
				"          fecha_fin as timestamp,",
				"          duracion as integer,",
				"          subidos as long,",
				"          descargados as long,",
				"          total as long,",
				"          id_origen_cinco as integer,",
				"          id_fecha_cinco as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id_origen_cinco,",
				"          id_fecha_cinco",
				"     )) ~> sink1"
			]
		}
	}
}