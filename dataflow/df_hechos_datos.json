{
	"name": "df_hechos_datos",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_datos",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_numero",
						"type": "DatasetReference"
					},
					"name": "Dimorigen"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_fecha",
						"type": "DatasetReference"
					},
					"name": "Dimfecha"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_datos",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          fecha_inicio as timestamp,",
				"          fecha_fin as timestamp,",
				"          numero as string,",
				"          imei as string,",
				"          rbs_utilizada as string,",
				"          celda as string,",
				"          latitud as decimal(10,7),",
				"          longitud as decimal(10,7),",
				"          subidos as integer,",
				"          descargados as integer,",
				"          total as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT\\n  CAST(FECHA_INICIO AS DATE) AS FECHA_KEY, \\n  NUMERO,\\n  IMEI,\\n  SUBIDOS,\\n  DESCARGADOS,\\n  TOTAL\\nFROM SILVER.DATOS\\nWHERE FECHA_INICIO IS NOT NULL\\n',",
				"     format: 'query') ~> source1",
				"source(output(",
				"          id_origen_cinco as integer,",
				"          numero as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_origen_cinco, NUMERO AS numero\\nFROM GOLD_CINCO.dim_origen_cinco\\n',",
				"     format: 'query') ~> Dimorigen",
				"source(output(",
				"          id_fecha as integer,",
				"          fecha as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT id_fecha, FECHA AS fecha   \\nFROM GOLD.dim_fecha\\n',",
				"     format: 'query') ~> Dimfecha",
				"source1, Dimorigen lookup(source1@numero == Dimorigen@numero,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, Dimfecha lookup(fecha_inicio == fecha",
				"     && fecha_fin == fecha,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2 select(mapColumn(",
				"          id_origen_cinco,",
				"          id_fecha",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_sesion_cinco as integer,",
				"          fecha_inicio as timestamp,",
				"          fecha_fin as timestamp,",
				"          duracion as integer,",
				"          subidos as long,",
				"          descargados as long,",
				"          total as long,",
				"          id_origen_cinco as integer,",
				"          id_fecha_cinco as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id_origen_cinco,",
				"          id_fecha_cinco = id_fecha",
				"     )) ~> sink1"
			]
		}
	}
}